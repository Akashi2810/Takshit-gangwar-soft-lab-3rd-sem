//import java.util.Scanner;

import java.util.Scanner;
public class program8

{

public static void main(String args[]) {
    //three numbers input
Scanner sc = new Scanner(System.in);
    int a = sc.nextInt();
    int b = sc.nextInt();
    int c = sc.nextInt();
     
    int smallest;
     
    //find the smallest
    if(a<b) {
        if(c<a) {
            smallest = c;
        } else {
            smallest = a;
        }
    } else
     {
        if(b<c) {
            smallest = b;
        } else
         {
            smallest = c;
         }
        }
     }
 
} 
    System.out.println(smallest + " is the smallest.");


}

